@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dpv: <http://www.w3.org/ns/dpv#> .
@prefix acl:  <http://www.w3.org/ns/auth/acl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

@base <https://example.com/odrl-ac> .

odrl-ac:
	a owl:Ontology ;
	rdfs:label "ODRL access control profile profile"@en .

:Access a odrl:Action, acl:Access .

:PersonalDataCategory a odrl:Asset, dpv:PersonalDataCategory .

:Purpose
    a odrl:RightOperand, dpv:Purpose, owl:NamedIndividual, skos:Concept ;
    rdfs:isDefinedBy odrl-ac: ;
    rdfs:label "Purpose for processing"@en ;
    skos:definition "Indicates the purpose for carrying out the processing of personal data."@en ;
    skos:note "Example: Academic research, access control or advertising."@en .
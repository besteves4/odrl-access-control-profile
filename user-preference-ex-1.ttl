@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix vcard:<http://www.w3.org/2006/vcard/ns#> . 
@prefix odrl-ac: <https://example.com/odrl-ac> .

:ex-1
    a odrl:Policy ;
    odrl:permission [
        a odrl:Permission ;
        odrl:assigner <https://beatriz.com/profile/card#me> ;
        odrl:target odrl-ac:Contact ;
        odrl:action odrl-ac:Read ;
        odrl:constraint [
            odrl:leftOperand odrl:purpose ;
            odrl:operator odrl:isA ;
            odrl:rightOperand odrl-ac:SellProductsToDataSubject
        ] 
    ] .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dpv: <http://www.w3.org/ns/dpv#> .
@prefix dpv-gdpr: <http://www.w3.org/ns/dpv-gdpr#> .
@prefix odrl-ac: <https://example.com/odrl-ac> .

:app-2
    rdfs:comment """App 2, as identified by their URL https://example-app-2.com, 
    requests permission to collect and produce a copy of the health records, 
    medical prescriptions and health history of users for the purpose of performing 
    academic research and make the anonymised information available to the scientific community.""" ;
    a odrl:Policy ;
    odrl:assignee <https://example-app-2.com> ;
    odrl:target dpv:HealthRecord, dpv:Prescription, dpv:HealthHistory ;
    odrl:permission [
        a odrl:Permission ;
        odrl:action dpv:Collect, dpv:Copy ;
        odrl:constraint [
        odrl:leftOperand dpv:Purpose ;
            odrl:operator odrl:isA ;
            odrl:rightOperand dpv:AcademicResearch 
        ]
    ] ;
    odrl:permission [
        a odrl:Permission ;
        Odrl:action dpv:Anonymise, dpv:MakeAvailable ;
        odrl:constraint [
            odrl:leftOperand dpv:Recipient ;
            odrl:operator odrl:isA ;
            odrl:rightOperand dpv:ThirdParty
        ]
    ] .